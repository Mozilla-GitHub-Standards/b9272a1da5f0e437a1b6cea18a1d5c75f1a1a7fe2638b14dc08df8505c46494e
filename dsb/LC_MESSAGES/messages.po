msgid "Required"
msgstr "Trěbny"

#: mozillians/phonebook/forms.py:164 mozillians/phonebook/forms.py:165
msgid "Save"
msgstr "Składowaś"

#: mozillians/phonebook/forms.py:206
msgid "Skills can only contain alphanumeric characters and +.:-."
msgstr "Zamóžnosći směju jano alfanumeriske znamuška a +.:- wopśimowaś."

#: mozillians/phonebook/forms.py:224
msgid "Location must be inside a country."
msgstr "Městno musy w kraju byś."

#: mozillians/phonebook/forms.py:232
msgid "Region must also be saved if city is saved."
msgstr "Region musy se teke składowaś, jolic město se składujo."

#: mozillians/phonebook/forms.py:238
msgid "Search for your country on the map."
msgstr "Pytaj swój kraj na kórśe."

#: mozillians/phonebook/forms.py:239
msgid "Country cannot be empty."
msgstr "Kraj njamóžo prozny byś."

#: mozillians/phonebook/forms.py:272
msgid "Email"
msgstr "E-mailowa adresa"

#: mozillians/phonebook/forms.py:278
